typ vector {u start, u length}

def putc(u char):
    yield {42,char}

def print(vector string):
    i = 0
    while i != string.length:
        putc(read(string.start+i))
        i = i + 1

def println(vector string):
    print(string)
    print("\n")

def main() -> (u):
    #var has same name as func!
    a = "hello, world!"
    println(a)
